<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.apotek">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_resize"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_resize"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".Activity.Checkout"></activity>
        <activity
            android:name=".Activity.StatusActivity"
            android:label="Keranjang Saya"
            android:parentActivityName=".Activity.MainActivity"></activity>
        <activity
            android:name=".Activity.EditProfile"
            android:label="Profile"
            android:parentActivityName=".Activity.MainActivity" />
        <activity
            android:name=".Activity.Pesan_ObatActivity"
            android:label="Shop"
            android:parentActivityName=".Activity.MainActivity" />
        <activity
            android:name=".Activity.MainActivity"
            android:label="Dashboard" />
        <activity android:name=".Activity.LoginActivity" />
        <activity android:name=".Activity.DaftarActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>